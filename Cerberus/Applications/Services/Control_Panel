/*
 ============================================================================================================
 + Find and Open :: Directory :: Applications :: Services Root Administration-Level
 ============================================================================================================
*/

$_FIND_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION			= "./Applications/Services";
$_OPEN_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION			= opendir($_FIND_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION);

/*
 ============================================================================================================
 + WHILE: Reading :: Directory :: Applications :: Services Root Administration-Level
 ============================================================================================================
*/

while (($_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION = readdir($_OPEN_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION))) {

/*
 ============================================================================================================
 + IF: Stop :: Exploit :: Local-File-Inclusion and Remote-File-Inclusion
 ============================================================================================================
*/

if ($_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION == "." || $_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION == ".." || $_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION == "index.php") {
/**
 * Do Nothing
**/
} else {

/*
 ============================================================================================================
 + IF: Internal Application :: Action :: Is: Application Module :: Administration
 ============================================================================================================
*/

if ($_GET["Action"] == "$_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION") {

/*
 ============================================================================================================
 + IF: Registered Member Account :: Administrator :: Access Level :: Is: >= 3
 ============================================================================================================
*/

if ($_GLOBAL_COOKIE_MEMBER_USERNAME && $_GLOBAL_COOKIE_MEMBER_PASSWORD != null && $_GLOBAL_MEMBER_ACCESS_LEVEL >= 5) {

										include_once "$_FIND_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION/$_READ_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION";

} else {

	echo ($_Message_Kernel_APPLICATION_SERVICES_APPLICATION_ACCESS_RESTRICTED_SERVICES);

} // [ + ] IF: Registered Member Account :: Cookies :: Credentials :: Exist, are Valid and Registered Member Account Access-Level Is: Services Root Administrator, >= 5

} // [ + ] IF: Internal Application: Action :: Is: Activated

} // [ + ] IF: Services Root Administration Applications Directory IS Anything ELSE Except The Services Root Administration Application

} // [ + ] WHILE: Reading :: The Services Root Administration Applications Directory

/*
 ============================================================================================================
 + Close :: Directory :: Applications :: Services Root Administration
 ============================================================================================================
*/

closedir($_OPEN_DIRECTORY_APPLICATIONS_SERVICES_ROOT_ADMINISTRATION);
