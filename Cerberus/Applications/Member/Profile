echo ("Streams :: Message: I have Deleted the Stream Directory Contents Successfully.<BR>");

echo ("Streams :: Message: Automatically Redirecting to the Registered Member Account Profile :: Streams... Wait.<BR>");

	header("Location: $_INTERNAL_FILE_KERNEL?$_INTERNAL_APPLICATION_MODULE_MEMBER=Profile&Name=$_Application_Module_Member_Profile_LIST_MEMBER_USERNAME&Display=Streams&$_GLOBAL_SYSTEM_SECURITY_CACHE_CIRCUMVENTION_HTR_RANDOM_STRING_GENERATION");

} else {

echo ("Streams :: Message: I was not able to Delete the Stream Directory Contents.");

} // [ + ] IF: Unlink :: Recursively :: Directory Contents :: Stream

} // [ + ] WHILE: Reading :: Directory Contents :: Registered Member Account :: Stream

echo ("Streams :: Message: Removing Stream Directory: $_Application_Module_Member_Profile_Streams_FIND_DIRECTORY_STREAMS_FILES_DELETE ... Wait.<BR>");

rmdir("$_Application_Module_Member_Profile_Streams_FIND_DIRECTORY_STREAMS_FILES_DELETE");

} else {

echo ("Streams :: Messsage: I cannot Delete this Stream because you are not the author of this Stream.");

} // [ + ] IF: Stream Author :: Is: Registered Member Account :: Owner

} else {

echo ("Message: You are not Logged-In. Please Authenticate into your Registered Member Account to see if you can Delete this Stream.");

} // [ + ] IF: Registered Member Account :: Cookies :: Credentials :: Exist AND ARE Valid

} // [ + ] IF: Streams Directory AND Files ARE NOT Streams Directory AND Files

} // [ + ] IF: Internal Application: Display :: Profile :: Name IS Registered Member Account Name :: Is: Activated

} // [ + ] WHILE: Reading Directory: Registered Member Account :: Streams :: Delete Stream

} // [ + ] IF: Internal Application: Display :: Profile :: Name :: Streams :: Delete :: Stream :: Is: Activated

/*
 ============================================================================================================
 +
 +
 + [ @ ] Internal Application :: Display :: Profile :: Name :: Streams :: Live :: Video Stream
 +
 +
 ============================================================================================================
*/

if ($_GET["Live"] == "VideoStream") {

if ($_GLOBAL_COOKIE_MEMBER_USERNAME && $_GLOBAL_COOKIE_MEMBER_PASSWORD != null) {

if ($_GLOBAL_COOKIE_MEMBER_USERNAME == "$_Application_Module_Member_Profile_LIST_MEMBER_USERNAME") {

echo ("
			Live Video Stream<HR>
			<OBJECT DATA=\"./System/Plug-Ins/Camera/$_GLOBAL_SYSTEM_PLUGIN_DIRECTORY_CAMERA/Live/Profile/Camera.php\" WIDTH=\"75%\" HEIGHT=\"265\></OBJECT>
");

} else {

echo ("Streams Message: This Registered Member Account Does Not Belong To You.");

} // [ + ] IF: Registered Member Account :: UserName :: DOES NOT Match Current Registered Member Account Profile UserName

} else {

echo ("Streams Message: Authentication Is Required In Order To Broadcast a Live Video Stream.<BR>");

} // [ + ] IF: Registered Member Account :: Credentials :: Exist AND ARE Valid

} // [ + ] IF: Internal Application: Display :: Profile :: Name :: Streams :: Live :: VideoStream :: Is: Activated

} // [ + ] IF: Internal Application: Display :: Profile :: Name :: Streams Is Activated

